if unknown cmbase: input cmbase fi
if unknown tipabase: input tipabase fi

font_identifier:="TIPXBSR"; font_size 10pt#;

Times_Compat:=false;
if Times_Compat: input tipatr fi

input brs10pt;

slant:=0;      % tilt ratio $(\Delta x/\Delta y)$
variant_g:=true;    % should an italic-style g be used?
low_asterisk:=false;    % should the asterisk be centered at the axis?
math_fitting:=false;    % should math-mode spacing be used?
%ligs:=2;
acc_height#:=70/36pt#; % ?
dot_height#:=70/36pt#; % 66.5
ldot_height#:=70/36pt#; % 66.5
%bx_kerning:=1;
sc.u#:=14/36pt#;
sc.letter_fit#:=2.4/36pt#;

generate tipx    % switch to the driver file
